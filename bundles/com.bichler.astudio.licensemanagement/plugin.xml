<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.4"?>
<plugin>
   <extension
         point="org.eclipse.ui.handlers">
	  <handler
            class="com.bichler.astudio.licensemanagement.handlers.OpenLicenseManagerHandler"
            commandId="com.bichler.astudio.commands.openlicensemanager">
      </handler>
   </extension>
   <extension
         point="org.eclipse.ui.commands">
      <command
            id="com.bichler.astudio.commands.openlicensemanager"
            name="Open Licencse Editor">
      </command>
   </extension>
   <extension
         point="org.eclipse.ui.editors">
      <editor
            class="com.bichler.astudio.licensemanagement.editor.LicenseManagerEditorPart"
            default="false"
            id="com.bichler.astudio.editors.licensemanager"
            name="LicenseEditor">
      </editor>
   </extension>

</plugin>
